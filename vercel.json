{
  "version": 2,
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/OneSignalSDKWorker.js",
      "dest": "app.py"
    },
    {
      "src": "/OneSignalSDK.sw.js",
      "dest": "app.py"
    },
    {
      "src": "/(.*)",
      "dest": "app.py"
    }
  ],
  "crons": [
    {
      "path": "/send_email",
      "schedule": "0 8 * * *"
    }
  ]
}
